{
    "swagger": "2.0",
    "info": {
        "description": "A car rental API utilizing payment gateway (Xendit) and Google Maps API. Made as a project for Hacktiv8",
        "title": "carStruck API",
        "contact": {
            "name": "Daniel Rahmanto",
